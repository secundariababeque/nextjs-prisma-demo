generator client {
    provider = "prisma-client-js"
    binaryTargets = ["native", "linux-musl", "linux-arm-openssl-1.1.x"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id        Int    @id @default(autoincrement())
    firstName String
    lastName  String
    avatar    String
    email     String

    @@map(name: "users")
}
